<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="customButton" parent="TextAppearance.AppCompat.Button">
        <item name="android:background">@color/colorAccent</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="bottomNavigationStyle" parent="Widget.MaterialComponents.BottomNavigationView">
       <item name="android:fontFamily">@font/questrial</item>
    </style>

    <style name="ShapeAppearanceOverlay.Button.Circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>